    }
  },
  "siteConfiguration": {
    "logo": { "text": "JPineDev" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Projects", "url": "/projects" },
        { "label": "Features", "url": "/#features" },
        { "label": "Github", "url": "/#github" }
